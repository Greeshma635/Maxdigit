using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace GreeshmaConsoleApp3
{
    class Program
    {
        static void Main(string[] args)
        {
            int num = 56897;
            int rev = 0;
            while (num>0)
            {
                    int result = num % 10;
                    rev= rev*10+result;
                    num /= 10;
               
            }

            Console.WriteLine($"{rev} : ");
        }
    }
}
