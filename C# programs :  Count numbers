namespace SaparateConsoleApp3
{
    class Program
    {
        static void Main(string[] args)
        {
            int num = 8851;
            int count = 0;
            while (num > 0)
            {

               int digit= num % 10;
                count++;
                num = num / 10;
            
            }
            Console.WriteLine(count);
        }
    }    
}
